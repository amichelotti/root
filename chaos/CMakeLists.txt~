include_directories(${CHAOS_INCLUDE_DIR})
ROOT_GENERATE_DICTIONARY(G__chaos *.h MODULE chaos LINKDEF LinkDef.h)
ROOT_LINKER_LIBRARY(chaos *.cxx G__chaos.cxx LIBRARIES Core ${CHAOS_LIBRARIES} DEPENDENCIES Net RIO)

MESSAGE(STATUS "Chaos include ${CHAOS_INCLUDE_DIR} .")
